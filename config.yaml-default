bind_ips:
  - "0.0.0.0/0"
  - "127.0.0.1/2"
blacklist_hosts:
  - "10.0.0.0/24"
exit_on_error: true
captured_text_newline_separator: "."
io_timeout_seconds: 30
print_config: true
file_logging: true
teams_logging: false
screen:
  print_ascii: true
  print_hex: false
  print_disconnect: false
file:
  log_ascii: false
  log_hex: true
  log_disconnect: false
teams:
  channel_url: ""
  log_ascii: false
  log_hex: false
  log_disconnect: false